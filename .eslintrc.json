{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "ignorePatterns": ["*.config.js"],
  "rules": {
    "max-len": ["error", {
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
      }],
    "strict": "warn"
  }
}
