{
  "name": "@aloskutov/eleventy-plugin-external-links",
  "version": "2.1.1",
  "description": "Eleventy external links plugin",
  "prettier": "@aloskutov/prettier-config",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "test": " node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "coverage": " node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "npx eslint ./src && npx jshint ./src"
  },
  "main": ".eleventy.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aloskutov/eleventy-plugin-external-links.git"
  },
  "keywords": [
    "11ty",
    "11ty-plugin",
    "eleventy",
    "eleventy-plugin",
    "plugin",
    "external-links"
  ],
  "author": "Alex Loskutov",
  "license": "MIT",
  "11ty": {
    "compatibility": ">=0.11"
  },
  "bugs": {
    "url": "https://github.com/aloskutov/eleventy-plugin-external-links/issues"
  },
  "devDependencies": {
    "@aloskutov/prettier-config": "^1.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "babel-jest": "^30.2.0",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.39.2",
    "eslint-config-google": "^0.14.0",
    "globals": "^17.2.0",
    "jest": "^30.2.0",
    "jshint": "^2.13.6"
  },
  "dependencies": {
    "node-html-parser": "^7.0.2"
  }
}
